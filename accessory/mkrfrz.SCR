for lay = Nextlay to Highestlayer!
	setlayer@ lay
	if layertype! <> 21 then goto notdrilllayer
	table = tooltable!
	number = 0;
	for i=1 to Highesttoolref! 
		set_current_toolref@ i
		' проверка на повторяемость номеров
		for j=0 to number
			if Toolnum! = numbers(j) AND Toolnum! <> 0 then
				MessageBox "Повторяются номера в таблице сверл!","Ошибка!!!",ERROR
				goto endscript
			end if
		next
		number=number+1
		numbers(number)=Toolnum!
		
		if tooltype! = 2 then goto freza

		for index=1 to lines%
			if d#(index)=toolsize! then exit for
		next
		if index>lines% then goto notoolintable
		if units! = 1 then
			nc_set_tool_infeed@ table,i,if#(index)*10/6
			nc_set_tool_outfeed@ table,i,of#(index)*10/6
		else
			nc_set_tool_infeed@ table,i,if#(index)*10/6
			nc_set_tool_outfeed@ table,i,of#(index)*10/6
		end if
		nc_set_tool_rpms@ table,i,r#(index)*1000
		goto notoolintable
freza:
		frezy(fre) = i
		fre = fre + 1
		nc_set_tool_infeed@ table,i,8.3333
		nc_set_tool_outfeed@ table,i,75
		nc_set_tool_rpms@ table,i,30000
		ts# = toolsize!
		if ts# < 1 then
			ts# = 1 ' проверка на маленькие фрезы'
		end if
		nc_set_tool_feedrate@ table,i,ts#*10/6 'преобразование
notoolintable:
	next
notdrilllayer:
next

return

