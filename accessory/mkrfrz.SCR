	end if
	print #1, USING "M47, ###*###  ##";X%;Y%;Z#
	nx# = nullx#-xprob#
	ny# = nully#-Z#
	if prob% = 2 AND fr <> 1 AND hh = 2 then
		if sz230x395 = 1 then
